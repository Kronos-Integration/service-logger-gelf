{
  "name": "kronos-service-logger-gelf",
  "version": "0.0.0-semantic-release",
  "description": "forwards log entries to a GELF server",
  "main": "dist/module.js",
  "module": "src/gelf-logger-service.js",
  "scripts": {
    "cover": "nyc --temp-directory build/nyc ava",
    "test": "ava",
    "semantic-release": "semantic-release",
    "docs": "jsdoc2md --configure doc/jsdoc.json -l off -t doc/README.hbs -f src/*.js >README.md",
    "pretest": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "precover": "rollup -c tests/rollup.config.js",
    "prepare": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kronos-Integration/kronos-service-logger-gelf.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-service-logger-gelf/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-service-logger-gelf#readme",
  "dependencies": {
    "gelf": "^2.0.0",
    "kronos-service": "^4.8.2",
    "model-attributes": "^3.0.10"
  },
  "devDependencies": {
    "jsdoc-to-markdown": "^3.0.2",
    "markdown-doctest": "^0.9.1",
    "nyc": "^11.4.0",
    "rollup": "^0.52.2",
    "semantic-release": "^11.0.2",
    "jsdoc-babel": "^0.3.0",
    "ava": "^0.24.0",
    "rollup-plugin-multi-entry": "^2.0.2",
    "xo": "^0.19.0",
    "babel-preset-env": "^1.6.1"
  },
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=8.9.3"
  },
  "keywords": [
    "kronos-service"
  ],
  "nyc": {
    "include": [
      "dist/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./coverage"
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  },
  "xo": {
    "space": true
  },
  "ava": {
    "files": [
      "build/*-test.js"
    ],
    "presets": [
      "env"
    ],
    "require": [
      "babel-register"
    ]
  }
}
