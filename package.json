    "semantic-release": "^19.0.2"
  },
  "engines": {
    "node": ">=16.15.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/service-logger-gelf.git"
  },
  "bugs": {
