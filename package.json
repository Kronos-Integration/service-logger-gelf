{
  "name": "kronos-service-logger-gelf",
  "version": "0.0.0-semantic-release",
  "description": "forwards log entries to a GELF server",
  "main": "dist/module.js",
  "module": "src/GelfLoggerService.js",
  "scripts": {
    "cover": "nyc npm test",
    "test": "mocha tests/*_test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js >README.md",
    "prepublish": "rollup --output=dist/module.js -c -- src/GelfLoggerService.js",
    "pretest": "npm run prepublish",
    "posttest": "markdown-doctest",
    "precover": "npm run prepublish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/kronos-service-logger-gelf.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-service-logger-gelf/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-service-logger-gelf#readme",
  "dependencies": {
    "gelf": "^1.0.3",
    "kronos-service": "^4.5.4",
    "model-attributes": "^3.0.8"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "cz-conventional-changelog": "^2.0.0",
    "jsdoc-to-markdown": "^3.0.0",
    "markdown-doctest": "^0.9.1",
    "mocha": "^3.2.0",
    "nyc": "^10.1.2",
    "rollup": "^0.41.5",
    "semantic-release": "^6.3.6"
  },
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=6.9.4"
  },
  "keywords": [
    "kronos-service"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "nyc": {
    "include": [
      "dist/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./coverage"
  }
}
