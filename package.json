{
  "name": "kronos-service-logger-gelf",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/module.js",
  "module": "src/gelf-logger-service.js",
  "description": "forwards log entries to a GELF server",
  "keywords": [
    "kronos-service"
  ],
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "nyc --temp-directory build/nyc ava",
    "test": "ava",
    "semantic-release": "semantic-release",
    "docs": "documentation readme src/gelf-logger-service.js --section=API",
    "pretest": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "precover": "rollup -c tests/rollup.config.js",
    "prepare": "rollup -c",
    "lint": "documentation lint src/gelf-logger-service.js",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {
    "gelf": "^2.0.0",
    "kronos-service": "^4.16.0",
    "model-attributes": "^3.0.10"
  },
  "devDependencies": {
    "markdown-doctest": "^0.9.1",
    "nyc": "^12.0.2",
    "rollup": "^0.62.0",
    "semantic-release": "^15.6.0",
    "ava": "^0.25.0",
    "rollup-plugin-multi-entry": "^2.0.2",
    "documentation": "^8.0.0",
    "travis-deploy-once": "^5.0.1"
  },
  "engines": {
    "node": ">=10.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/kronos-service-logger-gelf.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-service-logger-gelf/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-service-logger-gelf#readme",
  "ava": {
    "files": [
      "build/*-test.js"
    ],
    "presets": [
      "env"
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "dist/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./build/coverage"
  },
  "xo": {
    "space": true
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
